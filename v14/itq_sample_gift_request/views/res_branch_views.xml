<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="res_branch_view_form" model="ir.ui.view">
        <field name="name">res.branch.view.form.inherit.stock.request</field>
        <field name="model">res.branch</field>
        <field name="inherit_id" ref="itq_branch_base.view_branch_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='website']" position="after">
                <field name="apply_sample_limitation"/>
            </xpath>

        </field>
    </record>

</odoo>



