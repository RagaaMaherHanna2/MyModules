<?xml version="1.0" encoding="UTF-8" ?>
<templates xml:space="preserve">
  <t t-inherit="itq_my_hr_dashboard.MyDashboardTemp" name="MyDashboardBenefitsTemp" t-inherit-mode="extension">
    <xpath expr="//div[@name='employee_jobs']" position="after">
      <!-- Projects -->
      <div class="row main-section" name="employee_projects">
        <div class="col-sm-12 col-lg-12 p-0 mb-3">
          <div class="col-sm-12 col-lg-12  graph_view"
            style="padding:0">
            <div class="card">
              <div class="card-header">Projects</div>
              <div class="card-body">
                <div class="text-color hr-chart-1">
                  <div class="oh-card-body pb-0">
                    <h4 class="mb-0">
                    </h4>
                    <div class="oh-ribbon"><span>Projects</span></div>
                  </div>
                  <div class="emp_projects_pie" />
                </div>
              </div>
            </div>

          </div>
        </div>
      </div>
    </xpath>
  </t>
</templates>


