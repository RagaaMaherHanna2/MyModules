            <field name="padding">6</field>
            <field name="implementation">no_gap</field>
            <field name="number_increment">1</field>
        </record>
        <record id="work_procedure_seq" model="ir.sequence">
            <field name="name">Work Procedure Seq</field>
            <field name="code">work.procedure.seq</field>
            <field name="active">TRUE</field>
            <field name="padding">6</field>
            <field name="implementation">no_gap</field>
            <field name="number_increment">1</field>
        </record>
    </data>
</odoo>
