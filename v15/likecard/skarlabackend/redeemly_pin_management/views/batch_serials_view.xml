<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="batch_serials_view_tree" model="ir.ui.view">
            <field name="name">batch.serials.view.tree</field>
            <field name="model">batch.serials</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="batch_sequence"></field>
<!--                    <field name="batch_file"></field>-->
                    <field name="state"></field>
                </tree>
            </field>
        </record>
        <record id="batch_serials_view_form" model="ir.ui.view">
            <field name="name">batch.serials.view.form</field>
            <field name="model">batch.serials</field>
            <field name="arch" type="xml">
                <form edit="false">
                    <sheet>
                        <group>
                            <field name="batch_sequence"></field>
<!--                        <field name="batch_file"></field>-->
                            <field name="state"></field>

                        </group>
                    </sheet>
                </form>
            </field>
        </record>
    </data>
</odoo>