<odoo>
    <data noupdate="1">
        <record id="itq_event_request_sequence" model="ir.sequence">
            <field name="name">Event request sequence</field>
            <field name="code">event.request.sequence</field>
            <field eval="1" name="number_increment"/>
            <field name="implementation">no_gap</field>
        </record>
        <record id="itq_visit_request_sequence" model="ir.sequence">
            <field name="name">Visit request sequence</field>
            <field name="code">visit.request.sequence</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field name="implementation">no_gap</field>
        </record>

        <record id="itq_event_permission_sequence" model="ir.sequence">
            <field name="name">Event permission sequence</field>
            <field name="code">event.permission.sequence</field>
            <field eval="1" name="number_increment"/>
            <field name="implementation">no_gap</field>
        </record>
    </data>
</odoo>